{
  "manifest_version": 3,
  "name": "CN Wristband Scanner",
  "version": "1.1",
  "description": "Use RFID wristbad and scanner to scann in ninjas",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://cn.mystudio.io/attendance/*",
        "https://can.mystudio.io/*"
      ],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
